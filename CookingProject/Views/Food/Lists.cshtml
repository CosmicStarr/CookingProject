@model FoodListView





<div class="container">
    <h1 class="offsets text-center">Store</h1>
    <hr />
    <h3 class="text-center">@Model.CurrentCategory</h3>
    <div>
        <div class="card-columns">
            @foreach (var Product in Model.Foods)
            {
                <partial name="_FoodCard" model="Product" />
            }
        </div>
    </div>

</div>